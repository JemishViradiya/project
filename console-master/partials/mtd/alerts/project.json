{
  "root": "partials/mtd/alerts",
  "sourceRoot": "partials/mtd/alerts/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["partials/mtd/alerts/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/partials/mtd/alerts"],
      "options": {
        "jestConfig": "partials/mtd/alerts/jest.config.js",
        "passWithNoTests": true
      }
    },
    "serve": {
      "executor": "./tools/executors/partial-dev-server:serve"
    }
  },
  "tags": ["scope:partial", "scope:mtd"]
}
